    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "react-snap": "^1.23.0",
    "typescript": "^4.1.3"
